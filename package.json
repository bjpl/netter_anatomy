    "dexie": "^4.0.8",
    "lucide-react": "^0.562.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "tailwind-merge": "^2.5.4",
    "three": "^0.169.0",
    "ts-fsrs": "^4.4.0",
    "zustand": "^4.5.5"
